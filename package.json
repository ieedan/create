{
	"name": "@ieedan/templates",
	"version": "1.0.0",
	"description": "Project templates to quickly spin up projects the way I like to build them.",
	"main": "./dist/index.mjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"bin": "./bin.mjs",
	"files": [
		"bin.mjs",
		"dist"
	],
	"publishConfig": {
		"scripts": {},
		"devDependencies": {}
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs"
		}
	},
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"format": "npx prettier . --write",
		"build": "unbuild",
		"check": "tsc --noEmit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ieedan/templates.git"
	},
	"keywords": [
		"templates"
	],
	"author": "Aidan Bleser",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ieedan/templates/issues"
	},
	"homepage": "https://github.com/ieedan/templates#readme",
	"dependencies": {
		"chalk": "^5.3.0",
		"detect-package-manager": "^3.0.2",
		"execa": "^9.3.0",
		"fs-extra": "^11.2.0"
	},
	"devDependencies": {
		"@types/fs-extra": "^11.0.4",
		"prettier": "^3.3.2",
		"unbuild": "^2.0.0"
	}
}
